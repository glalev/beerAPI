{
    "beers": {
        "belongsTo": ["brewery", "country", "category", "style"],
        "hasOneOrMany": ["comments", "ratings"]
    },
    "breweries": {
        "belongsTo": ["country"],
        "hasOneOrMany": ["geocodes", "beers"]
    },
    "categories": {},
    "comments": {
        "belongsTo": ["user", "beer"],
        "foreignFields": ["users.username, users.picture AS userPicture"]
    },
    "countries": {},
    "geocodes": {},
    "ratings": {},
    "styles": {},
    "users": {
        "hasOneOrMany": ["comments"]
    }
}
